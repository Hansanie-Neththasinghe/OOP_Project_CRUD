<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>sound Waves</display-name>
  <welcome-file-list>
    <welcome-file>songinsert.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
		 	<servlet-name>login</servlet-name>
		 	<servlet-class>com.SoundWave.servlet.artist.LoginServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>login</servlet-name>
		 	<url-pattern>/log</url-pattern>
		 </servlet-mapping>
		 
		 
		 <servlet>
		 	<servlet-name>songinsert</servlet-name>
		 	<servlet-class>com.SoundWave.servlet.artist.SongInsertServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 <servlet-name>songinsert</servlet-name>
		 <url-pattern>/insert</url-pattern>
		 </servlet-mapping>
		 
		 
		 <servlet>
		 	<servlet-name>updatesong</servlet-name>
		 	<servlet-class>com.SoundWave.servlet.artist.UpdateSongServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 <servlet-name>updatesong</servlet-name>
		 <url-pattern>/update</url-pattern>
		 </servlet-mapping>
		 
		 
		 <servlet>
		 	<servlet-name>deletesong</servlet-name>
		 	<servlet-class>com.SoundWave.servlet.artist.DeleteSongServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 <servlet-name>deletesong</servlet-name>
		 <url-pattern>/delete</url-pattern>
		 </servlet-mapping>
</web-app>